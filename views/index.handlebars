<h1>Eat-Da Burger</h1>
<h2>Eat until You die</h2>
<h2>It is more than you can imagine</h2>

<ul>
  {{#each burgers}}
  	<li>
  	   <p>ID: {{this.id}}</p>
         {{#unless this.devoured}}
       <p>Burger: {{this.burger_name}}</p>
  	</li>
  {{/each burgers}}
</ul>

<form action="/" method="POST">
	<textarea type="text" name="wish"></textarea>

	<button type="submit">Submit</button>
</form>
